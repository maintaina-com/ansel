{
    "name": "horde/ansel",
    "description": "Photo gallery application",
    "type": "horde-application",
    "homepage": "https://www.horde.org/apps/ansel",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Michael J Rubinsky",
            "email": "mrubinsk@horde.org",
            "role": "lead"
        },
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "version": "4.0.0",
    "time": "2020-10-27",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "dev-master",
        "php": "^5.3 || ^7",
        "horde/content": "dev-master",
        "horde/horde": "dev-master",
        "horde/auth": "dev-master",
        "horde/autoloader": "dev-master",
        "horde/core": "dev-master",
        "horde/date": "dev-master",
        "horde/db": "dev-master",
        "horde/exception": "dev-master",
        "horde/form": "dev-master",
        "horde/group": "dev-master",
        "horde/image": "dev-master",
        "horde/mime": "dev-master",
        "horde/nls": "dev-master",
        "horde/perms": "dev-master",
        "horde/prefs": "dev-master",
        "horde/serialize": "dev-master",
        "horde/share": "dev-master",
        "horde/support": "dev-master",
        "horde/text_filter": "dev-master",
        "horde/url": "dev-master",
        "horde/util": "dev-master",
        "horde/view": "dev-master",
        "horde/vfs": "dev-master",
        "ext-gettext": "*",
        "ext-hash": "*"
    },
    "suggest": {
        "horde/service_urlshortener": "^2",
        "horde/service_twitter": "^2",
        "horde/service_facebook": "^2",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-libpuzzle": "*",
        "ext-php-facedetect": "*"
    },
    "autoload": {
        "psr-0": {
            "Ansel": "lib/"
        }
    },
    "bin": [
        "bin/ansel",
        "bin/ansel-convert-sql-shares-to-sqlng",
        "bin/ansel-garbage-collection"
